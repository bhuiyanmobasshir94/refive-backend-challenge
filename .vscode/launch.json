    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver",
                "localhost:9501",
            ],
            "preLaunchTask": "docker-compose-up",
            "django": true,
            "justMyCode": true,
            "env": {
                "AWS_PROFILE": "refive",
                "AWS_DEFAULT_REGION": "ap-southeast-2",
                "DJANGO_DEBUG": "true",
            },
        }
    ]
}